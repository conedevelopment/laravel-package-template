{
    "name": "conedevelopment/package-name",
    "description": "Package Description",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "laravel",
        "package"
    ],
    "authors": [
        {
            "name": "Cone Development",
            "email": "hello@conedevelopment.com"
        }
    ],
    "require": {
        "php": "^8.3 || ^8.4",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/laravel": "^12.0",
        "laravel/pint": "^1.6",
        "mockery/mockery": "^1.6",
        "larastan/larastan": "^3.4",
        "phpunit/phpunit": "^11.5.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Cone\\Package\\": "src/",
            "Cone\\Package\\Database\\Seeders\\": "database/seeders/",
            "Cone\\Package\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cone\\Package\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v1-dev"
        },
        "laravel": {
            "providers": [
                "Cone\\Package\\PackageServiceProvider"
            ]
        }
    }
}
